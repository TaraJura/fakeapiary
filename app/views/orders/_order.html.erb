<div id="<%= dom_id order %>">
  <p>
    <strong>Order number:</strong>
    <%= order.order_number %>
  </p>

  <p>
    <strong>Order number web:</strong>
    <%= order.order_number_web %>
  </p>

  <p>
    <strong>Note:</strong>
    <%= order.note %>
  </p>

  <p>
    <strong>Pdf docs:</strong>
    <%= order.pdf_docs.size %>
  </p>

  <p>
    <strong>Total:</strong>
    <%= order.total %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= order.currency %>
  </p>

  <p>
    <strong>Business unit:</strong>
    <%= order.business_unit_id %>
  </p>

</div>
