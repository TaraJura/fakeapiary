<div id="<%= dom_id address %>">
  <p>
    <strong>Name:</strong>
    <%= address.name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= address.company %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= address.street %>
  </p>

  <p>
    <strong>City:</strong>
    <%= address.city %>
  </p>

  <p>
    <strong>Zip:</strong>
    <%= address.zip %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= address.country %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= address.phone %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= address.email %>
  </p>

</div>
